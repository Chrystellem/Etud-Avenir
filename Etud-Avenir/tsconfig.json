{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "allowJs": true,
    "target": "es5",
    "module": "CommonJS",
    "jsx": "react",
    "moduleResolution": "node"
  },
  "include": [
    "wwwroot/src",
    "wwwroot/js"
  ],
  "exclude": [
    "node_modules"
  ]
}
